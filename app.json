{
  "expo": {
    "name": "udacifitness",
    "description": "Udacity React Nanodegree Project",
    "slug": "udacifitness",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://tylermcginnis.com/udacifitness-512.png",
    "notification": {
      "icon": "https://tylermcginnis.com/udacifitness-45.png"
    },
    "splash": {
      "image": "https://tylermcginnis.com/udacifitness-512.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.udacifitness.exp"
    },
    "android": {
      "package": "org.udacifitness.exp"
    },
    "web": {
      "build": {
        "babel": {
          "include": [
            "udacifitness-calendar",
            "deprecated-react-native-listview"
          ]
        }
      }
    }
  },
  "name": "udacifitness"
}
